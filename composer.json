{
    "name": "shovedev/shove-laravel",
    "description": "Laravel adapter for Shove.dev",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Shove\\Laravel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shove\\Laravel\\Tests\\": "tests",
        }
    },
    "authors": [
        {
            "name": "Hemp",
            "email": "hemp@hey.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.8",
        "saloonphp/saloon": "^3.0",
        "shovedev/shove-php": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^8.0"
    },
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../shove-php"
        }
    ]
}
    "extra": {
        "laravel": {
            "providers": [
                "Shove\\Laravel\\Providers\\ServiceProvider"
            ]
        }
    }
}