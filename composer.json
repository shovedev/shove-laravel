{
  "name": "shovedev/shove-laravel",
  "description": "Laravel adapter for Shove.dev",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Shove\\Laravel\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Shove\\Laravel\\Tests\\": "tests"
    }
  },
  "authors": [
    {
      "name": "Hemp",
      "email": "hemp@hey.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "guzzlehttp/guzzle": "^7.8",
    "saloonphp/saloon": "^3.0",
    "shovedev/shove-php": "dev-main",
    "illuminate/queue": "^10.0|^11.0|^12.0",
    "illuminate/container": "^10.0|^11.0|^12.0",
    "illuminate/contracts": "^10.0|^11.0|^12.0",
    "illuminate/support": "^10.0|^11.0|^12.0",
    "illuminate/http": "^10.0|^11.0|^12.0",
    "illuminate/testing": "^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "mockery/mockery": "^1.6"
  },
  "config": {
    "allow-plugins": {
      "kylekatarnls/update-helper": true
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "../shove-php"
    }
  ]
}
